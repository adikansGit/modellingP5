
/*---------------------------------------------------------------------------------------------------------------------------------------
Set the electrical and thermal efficiency factors of Cogen
---------------------------------------------------------------------------------------------------------------------------------------*/
param eff_cogen_el := 0.40;
param eff_cogen_heat := 0.35;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set the maximum scaling factor of Cogen
---------------------------------------------------------------------------------------------------------------------------------------*/
let  Fmax['Cogen'] := 40;

/*---------------------------------------------------------------------------------------------------------------------------------------
Set the reference size of shaft power (will be scaled with mult_t) 		
---------------------------------------------------------------------------------------------------------------------------------------*/
let Flowout['Electricity','Cogen']:=1000;

/*---------------------------------------------------------------------------------------------------------------------------------------
Set the cinv2 of the Cogen  
---------------------------------------------------------------------------------------------------------------------------------------*/
let cinv1["Cogen"]:= 4500000;
let cinv2["Cogen"]:= 500000;

/*---------------------------------------------------------------------------------------------------------------------------------------
Set the natural gas requirement and the heating output
---------------------------------------------------------------------------------------------------------------------------------------*/
let Flowin['Natgas','Cogen'] := Flowout['Electricity','Cogen'] / eff_cogen_el;
let Qheatingsupply['Cogen'] := Flowin['Natgas','Cogen']*eff_cogen_heat;
let Tminheating['Cogen'] := 120;
